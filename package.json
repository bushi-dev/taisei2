{
	"name": "taisei2",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"start": "vite",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"predeploy": "npm run build",
		"deploy": "npm run build && gh-pages-clean && gh-pages -d dist && echo https://bushi-dev.github.io/taisei2/",
		"prepare": "husky",
		"test": "echo \"Error: no test specified\" && exit 0",
		"format": "biome check --apply src"
	},
	"dependencies": {
		"@types/react-router-dom": "^5.3.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^7.1.1"
	},
	"husky": {
		"hooks": {
			"pre-push": "npm run deploy"
		}
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.10.6",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"gh-pages": "^6.1.0",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"typescript": "~5.6.2",
		"vite": "^6.0.5"
	}
}
